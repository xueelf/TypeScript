{
  "name": "typescript-template",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "format": "prettier **/*.ts --write",
    "lint": "eslint **/*.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@eslint/js": "latest",
    "eslint": "latest",
    "husky": "latest",
    "prettier": "latest",
    "typescript-eslint": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devEngines": {
    "runtime": {
      "name": "bun",
      "onFail": "error"
    },
    "packageManager": {
      "name": "bun",
      "onFail": "error"
    }
  },
  "private": true
}
